{
  "name": "srmklive/chargify",
  "type": "library",
  "description": "PHP Client for Chargify API.",
  "keywords": ["http", "rest", "web service", "chargify", "laravel chargify"],
  "license": "MIT",
  "authors": [
    {
      "name": "Raza Mehdi",
      "email": "srmk@outlook.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Srmklive\\Chargify\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Srmklive\\Chargify\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.2",
    "guzzlehttp/guzzle": "~7.0",
    "illuminate/support": "~6.0|~7.0|~8.0",
    "nesbot/carbon": "~2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0|^9.0",
    "symfony/var-dumper": "~5.0"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Srmklive\\Chargify\\Providers\\ChargifyServiceProvider"
      ],
      "aliases": {
        "PayPal": "Srmklive\\Chargify\\Facades\\Chargify"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
